{
  "name": "frontend",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bunx --bun vite dev",
    "build": "bunx --bun vite build",
    "preview": "bunx --bun vite preview",
    "prepare": "bunx --bun svelte-kit sync || echo ''",
    "check": "bunx --bun svelte-kit sync && bunx --bun svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "bunx --bun svelte-kit sync && bunx --bun svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "bunx --bun prettier --write .",
    "lint": "bunx --bun prettier --check . && bunx --bun eslint ."
  },
  "dependencies": {
    "@learn-language/shared": "workspace:*",
    "hono": "^4.7.5"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@iconify-json/ion": "^1.2.2",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@types/youtube": "^0.1.1",
    "backend": "workspace:*",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "unplugin-icons": "^22.1.0",
    "vite": "^6.0.0"
  }
}
